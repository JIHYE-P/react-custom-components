ct\";\r\nimport styled, {keyframes} from 'styled-components';\r\n\r\nconst Button = styled.button`\r\n  position: relative;\r\n  min-width: 150px;\r\n  margin: 8px;\r\n  font-family: 'Roboto', sans-serif;\r\n  font-weight: 500;\r\n  line-height: 1.5;\r\n  border-radius: 5px;\r\n  color: #fff;\r\n  overflow: hidden;\r\n  user-select: none;\r\n  border: none;\r\n  outline: none;\r\n  cursor: pointer;\r\n  box-shadow: 0px 3px 1px -2px rgba(0,0,0,0.2), 0px 2px 2px 0px rgba(0,0,0,0.14), 0px 1px 5px 0px rgba(0,0,0,0.12);\r\n  ${({size}) => {\r\n    switch(size){\r\n    case 'sm': \r\n    return 'padding: 10px 18px; font-size: 14px';\r\n    case 'md': \r\n    return 'padding: 13px 22px; font-size: 16px';\r\n    case 'lg': \r\n    return 'padding: 15px 26px; font-size: 18px';\r\n    }\r\n  }};\r\n  background: ${({color}) => color ? color : '#1976d2'};\r\n  &:hover {\r\n    box-shadow: 0px 2px 4px -1px rgba(0,0,0,0.2), 0px 4px 5px 0px rgba(0,0,0,0.14), 0px 1px 10px 0px rgba(0,0,0,0.12);\r\n  }\r\n`;\r\n\r\nconst fadeIn  = keyframes`\r\n  0% {\r\n    opacity: 0;\r\n  }\r\n  25% {\r\n    opacity: 1;\r\n  }\r\n  100% {\r\n    opacity: 0;\r\n    transform: scale(1);\r\n  }\r\n`;\r\n\r\nconst Ripple = styled.span`\r\n  display: block;\r\n  width: 0;\r\n  height: 0;\r\n  left: 0;\r\n  top: 0;\r\n  position: absolute;\r\n  border-radius: 100%;\r\n  background: rgba(255,255,255,0.35);\r\n  transform-origin: center;\r\n  transform: scale(0);\r\n  opacity: 0;\r\n  animation: ${fadeIn} 0.5s;\r\n`;\r\n\r\nconst SPEED = 500;\r\nlet i = 0;\r\n\r\nconst RippleButton = ({size = 'md', color, children, onClick, ...props}) => {\r\n  const [ripple, setRipple] = useState([]);\r\n  const button = useRef();\r\n  const handleClick = ({clientX, clientY}) => {\r\n    onClick?.();\r\n    const {width, left, top} = button.current.getBoundingClientRect();\r\n    const circleProperties = {\r\n      key: `circle-${i++}`,\r\n      width: width*2,\r\n      height: width*2,\r\n      left: (clientX-left)-(width*2/2),\r\n      top: (clientY-top)-(width*2/2),\r\n    }\r\n    setRipple(circle => [...circle, circleProperties]);\r\n    setTimeout(setRipple, SPEED, prev => prev.slice(1));\r\n  }\r\n\r\n  return <Button ref={button} size={size} color={color} onClick={handleClick} {...props}>\r\n    <span>{children}</span>\r\n    {ripple.map(({key, ...styleProperties}) => \r\n      <Ripple key={key} style={styleProperties}></Ripple>\r\n    )}\r\n  </Button>\r\n}\r\n\r\nexport default RippleButton;",["86","87"],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\stories\\Button.js",[],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\components\\FormField.js",[],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\stories\\Header.js",[],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\utils\\context.js",[],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\stories\\Page.js",[],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\components\\FormField.stories.js",["88"],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\pages\\Form.stories.js",["89"],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\index.js",[],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\App.js",[],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\reportWebVitals.js",[],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\pages\\Form.js",["90","91"],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\pages\\Ripple.js",[],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\pages\\Main.js",[],"D:\\JIHYE\\2. Github\\+React\\react-custom-components\\src\\components\\Layout.js",[],{"ruleId":"92","severity":1,"message":"93","line":6,"column":1,"nodeType":"94","endLine":9,"endColumn":3},{"ruleId":"92","severity":1,"message":"93","line":5,"column":1,"nodeType":"94","endLine":8,"endColumn":3},{"ruleId":"92","severity":1,"message":"93","line":5,"column":1,"nodeType":"94","endLine":11,"endCo